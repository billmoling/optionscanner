# Copy this file to `.env` and fill in actual values for local development.
# Environment variables defined here are loaded by Docker Compose and the application.


TWS_USERID=
TWS_PASSWORD=
IAPI_CLIENT_ID=1            # overrides default if you need a different client id
TRADING_MODE=paper          # switch to live only when you intend to trade live
VNC_SERVER_PASSWORD=
READ_ONLY_API=no
IBKR_HOST=127.0.0.1
IBKR_PORT=4002
IBKR_CLIENT_ID=1
# Slack webhook used by SlackNotifier when sending alert messages.
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/XXX/YYY/ZZZ
SLACK_TEST_CHANNEL=#option-scanner-test
GEMINI_API_KEY=
IBKR_MARKET_DATA_TYPE=LIVE          # 1 = real-time, 2 = frozen, 3 = delayed, 4 = delayed frozen
DISABLE_PORTFOLIO_MANAGER=no     # set to yes/true to skip running the portfolio manager




